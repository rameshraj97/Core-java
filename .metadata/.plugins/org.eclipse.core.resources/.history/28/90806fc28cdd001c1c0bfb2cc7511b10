import javax.servlet.http.HttpSessionBindingEvent; import javax.servlet.http.HttpSessionBindingListener; public class User implements HttpSessionBindingListener{ @Override
public void valueBound (HttpSessionBindingEvent arg0) { System.out.println("Value bound "+ arge.getName()); System.out.println("Value : "+arge.getValue());
} @Override
public void valueUnbound (HttpSessionBindingEvent arg0) {
} private String userId; public String getUserId() {
return userId;
}
